{
  "name": "tomastrajan.com",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server/main.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve -o",
    "serve:ssr:dev": "ng run website:serve-ssr",
    "serve:ssr:prod": "npm run build:ssr && node dist/server/main.js",
    "build:ssr": "ng build --configuration production && npm run build:server",
    "build:server": "ng run website:server:production && mv ./dist/server/main*.js ./dist/server/main.js && mv ./dist/server/main*.js.map ./dist/server/main.js.map",
    "heroku-postbuild": "npm run build:ssr",
    "format:write": "prettier projects/**/*.{ts,json,md,html} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,html} --list-different",
    "prerender": "ng run website:prerender"
  },
  "engines": {
    "node": "14.x"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^14.0.3",
    "@angular/cdk": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/material": "^14.0.3",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/platform-server": "^14.0.3",
    "@angular/router": "^14.0.3",
    "@nguniversal/express-engine": "^14.0.2",
    "@sendgrid/mail": "^7.2.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "express-useragent": "^1.0.15",
    "node-fetch": "^2.6.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "^14.0.3",
    "@angular/language-service": "^14.0.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nguniversal/builders": "^14.0.2",
    "@types/express": "^4.17.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "ts-loader": "^8.0.4",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4",
    "webpack-cli": "^3.3.12"
  }
}
